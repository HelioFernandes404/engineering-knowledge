# GLPI CLI Configuration
# Copy this file to .env and fill with your credentials

# GLPI API endpoint URL (without trailing slash)
GLPI_URL=https://itsm.systemframe.com.br/apirest.php

# GLPI App Token (get from GLPI Setup > API > Clients)
GLPI_APP_TOKEN=your_app_token_here

# GLPI User Token (get from GLPI Setup > API > Users)
GLPI_USER_TOKEN=your_user_token_here
